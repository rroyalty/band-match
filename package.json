{
  "name": "bandwidth",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "./server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run start\" \"cd client && npm run start\"",
    "heroku-postinstall": "cd client && npm run build",
    "seed": "cd server && sequelize db:seed:all"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rroyalty/bandwidth.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rroyalty/bandwidth/issues"
  },
  "homepage": "https://github.com/rroyalty/bandwidth#readme",
  "dependencies": {
    "if-env": "^1.0.4",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "concurrently": "^6.1.0"
  }
}
